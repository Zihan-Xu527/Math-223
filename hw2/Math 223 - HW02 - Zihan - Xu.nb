Cell[TextData[{
 StyleBox["x=0 is an irregular singular point",
  FontWeight->"Bold"],
 " because of logarithm function.\n\nThe transformation ",
 Cell[BoxData[
  FormBox[
   RowBox[{"t", "=", 
    RowBox[{"1", "/", "x"}]}], TraditionalForm]],ExpressionUUID->
  "49ddd7cb-2ea6-4b1a-b9a7-1a1920dd8399"],
 " gives\n ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["t", "4"], TraditionalForm]],ExpressionUUID->
  "a8790f80-66dc-4289-a360-b4bc09a0fee3"],
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox["d", "2"], "y"}], 
    SuperscriptBox["dt", "2"]], TraditionalForm]],ExpressionUUID->
  "51de969f-9c00-4c6b-b7e2-2dfb49bb70d0"],
 " + 2",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["t", "3"], TraditionalForm]],ExpressionUUID->
  "92c007c8-e30f-4694-8813-336fa44b6f68"],
 Cell[BoxData[
  FormBox[
   FractionBox["dy", "dt"], TraditionalForm]],ExpressionUUID->
  "104ea27a-6c1e-475b-bc18-01d56e269193"],
 "= ",
 Cell[BoxData[
  FormBox["-", TraditionalForm]],ExpressionUUID->
  "41aad0a8-9bf7-42f2-861f-6a7f9b6c3c58"],
 "(log t)",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "aab8e22a-5862-40d0-815f-b3e3f3d4f6c4"],
 "   =>  ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{
     SuperscriptBox["d", "2"], "y"}], 
    SuperscriptBox["dt", "2"]], TraditionalForm]],ExpressionUUID->
  "21c7ce35-d9a5-4b8e-aaf8-86d5b2017534"],
 " + ",
 Cell[BoxData[
